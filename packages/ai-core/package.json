{
  "name": "@copilot/ai-core",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "pnpm run copy-docs && tsup src/index.ts --watch --dts --format esm",
    "build": "tsup src/index.ts --dts --format esm && pnpm run copy-docs",
    "copy-docs": "node ./scripts/copy-docs.mjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "echo \"(optional)\""
  },
  "dependencies": {
    "@copilot/shared": "workspace:*",
    "@langchain/core": "1.1.8",
    "@langchain/community": "1.1.1",
    "@langchain/openai": "1.2.0",
    "faiss-node": "^0.5.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
